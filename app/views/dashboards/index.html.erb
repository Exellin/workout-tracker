<div class="jumbotron">
  <h1>Welcome to the workout app</h1>
  <p>Record and share your workouts here</p>
</div>

<div class="row">
  <div class="col-md-10">
    <h2>List of Members</h2>
    <%= will_paginate @athletes, renderer: BootstrapPagination::Rails, 
        class: "pull-left paginate" %>
  </div>
</div>

<div class="row">
  <% @athletes.each do |athlete| %>
  <div class="col-md-10 well member">
    <%= athlete.full_name %>
  </div>
  <% end %>
</div>